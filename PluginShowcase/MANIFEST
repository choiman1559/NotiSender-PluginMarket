#(REQUIRED) Name of plugin, This name will be displayed at NotiSender
pluginName=PluginLibrary Showcase

#(REQUIRED) Author of plugin. Basically Github user name
author=Choiman1559

#E-Mail address for contact
contact=cuj1559@gmail.com

#(REQUIRED) Simple description of plugin
description=Example application for plugin development

#Github (or other repo host site) repo link of plugin, Blank this item if plugin is proprietary software
repositoryLink=https://github.com/choiman1559/NotiSender-PluginShowcase

#(REQUIRED) Download link that user can actually download the apk file
downloadLink=https://github.com/choiman1559/NotiSender-PluginShowcase/releases/latest

#(REQUIRED) Latest version of plugin, Only number and dot(.) allowed, Semantic versioning recommended
latestVersion=1.0.0

#Currently not used properties, default value is false
isWebApp=false

#(REQUIRED) Whether the icon to be displayed in NotiSender is a fluent icon, if true, "iconName" attribute is used, if false, "iconUrl" attribute is used
useFluentIcon=true

#Name of Fluent icon, Icon is always 24_regular style
iconName=window_dev_tools

#Link must be end with .jpg (.jpeg) or .png to get icon
iconUrl=https://someurl.com/blob/imageExample.jpg

#List of tags, each icon is seperated as ","
tag=Development,Example,Showcase

#License of plugin, Write "Proprietary" if plugin is proprietary software
license=GPL-3.0

#(REQUIRED) Android package name of plugin
packageName=com.noti.plugin.showcase

#Visibility in NotiSender
visible=true